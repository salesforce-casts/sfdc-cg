<apex:page controller="exploreDynamicSearch">
    <apex:form >
        <apex:inputText value="{! q }"/>
        
        <apex:commandButton value="Search" action="{! search }" reRender="outAccount" />
        Account <apex:inputCheckbox value="{! cbaccount}"/>
        Candidate <apex:inputCheckbox value="{! cbcandidate}"/>
        
    </apex:form>
    
    <apex:outputText rendered="{! IF(message !=null, true, false) }">{! message}</apex:outputText>
    
    
    <apex:outputPanel id="outAccount">
        <apex:pageBlock rendered="{! cbaccount}">
            <apex:pageBlockTable value="{! aList }" var="r" >
                <apex:column headervalue="Name" >
				<apex:outputText value="{!r['nane']}"/>
                </apex:column>


                
            </apex:pageBlockTable>
        </apex:pageBlock>
    </apex:outputPanel>
    
</apex:page>